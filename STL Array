#include <iostream>
#include<array>
using namespace std;
int main() {
    array<int,5> arr={2,3,1,4,5};
    array<int,0> arr1={};
    int size= arr.size();
    
    for(int i=0;i<size ;i++){
        cout<<arr[i]<<endl;
    }
    cout<<"Element at specific index:"<<arr.at(3)<<endl;
    cout<<"First Element :"<<arr.front()<<endl;
    cout<<"Last Element :"<<arr.back()<<endl;
    cout<<"Is 1st array empty(1) or not(0) :"<<arr.empty()<<endl;
    cout<<"Is 2nd array empty(1) or not(0) :"<<arr1.empty()<<endl;
  
    return 0;
}


//Output :
2
3
1
4
5
Element at specific index:4
First Element :2
Last Element :5
Is 1st array empty(1) or not(0) :0
Is 2nd array empty(1) or not(0) :1
