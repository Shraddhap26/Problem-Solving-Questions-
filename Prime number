//Determine the number is prime or not.
*****************Program****************
#include <iostream>
using namespace std;
int main() {
    int n1;
   cout<<"Enter number :";
   cin>>n1;
   for(int i=2;i<n1-1;i++){
       if(n1%i==0){
           cout<<"it is not a prime number";
           break;
       }
   }

    return 0;
}

------------Output--------------
Enter number :80
it is not a prime number
