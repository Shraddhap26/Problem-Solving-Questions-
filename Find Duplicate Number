//Write a program to find out duplicate number(v-10)
//my program
#include <iostream>
#include <iostream>
using namespace std;
int main() {
   int arr[5]={4,3,3,4,2};
 
   int len=sizeof(arr)/sizeof(arr[0]);
   cout<<len<<endl;
   //sorting
     for(int i=0;i<len;i+=1)
    {
        for(int j=i+1;j<len;j++){
             if(arr[i]>arr[j]){
          
               int temp=arr[i];
               arr[i]=arr[j];
               arr[j]=temp;
             }
        }
     
    }
    cout<<"Sorted array : ";
    for(int i=0;i<5;i++){
        cout<<arr[i]<<" ";
    }
    cout<<"\nDuplicate elements are:";
 //for Printing Duplicate numbers
    for(int i=0;i<len;i+=1)
    {
      if(arr[i]==arr[i+1]){
      cout<<arr[i]<<" ";
      }
    }
   
    return 0;
}

//Output :
5
Sorted array : 2 3 3 4 4 
Duplicate elements are:3 4 
