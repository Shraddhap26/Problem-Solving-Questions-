//Code to convert decimal to binary
#include <iostream>
using namespace std;
#include<math.h>
using namespace std;
int main() {
  int a=10;
  int i=0;
  int ans=0;
  while(a!=0){
      int bit=a&1;
      ans=(bit*pow(10,i))+ans;
      i++;
      a=a>>1;
  }
 cout<<ans;


    return 0;
}

//Output :
1010
