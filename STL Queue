//Queue : It is a LILO or FIFO data structure
#include <iostream>
#include<queue>
using namespace std;
int main() {
    
  queue<string> q1;
  q1.push("Shraddha");
  q1.push("Gauri");
  q1.push("Rutika");
  q1.push("Sonali");
  
  cout<<"Size of queue:"<<q1.size();
  cout<<endl<<"Front of queue:"<<q1.front();
  q1.pop();
  cout<<"\n\nAfter popping 1 element:"<<endl;
  cout<<endl<<"front of queue:"<<q1.front();
   cout<<endl<<"Back of queue:"<<q1.back();
  cout<<endl<<"Size of queue:"<<q1.size();
  cout<<endl;
  cout<<"\nPrint queue:\n";
   while (!q1.empty()) {
        cout << q1.front() <<" ";
        q1.pop();
    }
  return 0;
}

//Output :
Size of queue:4
Front of queue:Shraddha

After popping 1 element:

front of queue:Gauri
Back of queue:Sonali
Size of queue:3

Print queue:
Gauri Rutika Sonali 
