// Online C++ compiler to run C++ program online
#include <iostream>
#include<bits/stdc++.h>
using namespace std;


int checkpass(char str[],int n, int min){
    if(min>n){
        
        return 0;
    }
    if(str[0]-'0'>=0 && str[0]-'0'<=9)
    {
        
        return 0;
    }
    int cap=0,dig=0,a=0;
    
    while(a<n){
         
        if(str[a]==' ' || str[a]=='+'){
             
            return 0;
        }
        if(str[a]>=65 && str[a]<=90)
    {
         
        cap++;
         
    }
     if(str[a]-'0'>=0 && str[a]-'0'<=9)
    {
        
        dig++;
    }
    a++;
    
    }
    return cap>0 && dig>0;
    
}

int main() {
    int n,min=5;
    string str;
    getline(cin,str);
    char *c=&str[0];
    cout<<"Size"<<str.size();
    if(checkpass(c,str.size(), min))
    {
        cout<<"Valid";
    }else{
        cout<<"Invalid";
    }

    return 0;
}
