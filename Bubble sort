//Program
//First large element will be sorted
#include <iostream>
using namespace std;
void bubblesort(int arr[],int n){
    for (int i=0;i<n-1;i++){
        for(int j=0;j<n-1;j++){
            if(arr[j]>arr[j+1]){
           swap(arr[j],arr[j+1]);
       }
     }
    }
    for(int i=0;i<n;i++){
         cout<<arr[i]<<endl;
    }
}
int main() {
    int arr[7]={65,43,22,89,12,10,3};
   bubblesort(arr,7);
    return 0;
}

//Output :
3
10
12
22
43
65
89
