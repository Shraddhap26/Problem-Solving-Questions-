#include<bits/stdc++.h>
using namespace std;

// } Driver Code Ends
class Solution
{
  public:
    string modify (string s)
    {
        int count=0;
        //code here.
        string a;
        for(int i=0;i<s.length();i++){
            if (s[i]!=' '){
                a+=s[i];
            }
        }
        
        return a;
    }
};
