#Code :
#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
   int arr[]={0,2,3,0,1,0,5,3,0,4,1};
   int n=sizeof(arr)/sizeof(arr[0]);
   int index=0,it=0;
   for(int i=0;i<n;i++){
       if(arr[i]!=0){
            swap(arr[i],arr[index]);
         
           index++;
       }
       
   }
   for(int i=0;i<n;i++){
       cout<<arr[i]<<endl;
   }

    return 0;
}

Output:

2
3
1
5
3
4
1
0
0
0
0
